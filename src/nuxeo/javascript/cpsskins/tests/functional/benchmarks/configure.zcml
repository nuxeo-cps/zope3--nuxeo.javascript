<configure
    xmlns:browser="http://namespaces.zope.org/browser">

  <browser:page
      for="*"
      layer="cpsskins.lib.cpsskins.tests.cpsskinsTest"
      name="cpsskins_benchmarks.html"
      permission="zope.Public"
      template="cpsskins_benchmarks.pt"
  />

  <browser:pages
      layer="cpsskins.lib.cpsskins.tests.cpsskinsTest"
      for="*"
      class=".browser.Views"
      permission="zope.Public">

    <browser:page
        name="getData1"
        attribute="getData1"
    />

    <browser:page
        name="setData1"
        attribute="setData1"
    />

  </browser:pages>

</configure>
